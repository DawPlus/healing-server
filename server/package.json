{
  "name": "forest-system-server",
  "version": "1.0.0",
  "description": "Forest Healing System Server",
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npx nodemon server.js",
    "dev:client": "cd ../client && yarn start",
    "dev:server": "cross-env NODE_ENV=development npx nodemon server.js",
    "dev:both": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "start": "pm2 start server.js --name forest-healing",
    "stop": "pm2 stop forest-healing",
    "restart": "pm2 restart forest-healing",
    "logs": "pm2 logs forest-healing",
    "build": "cd ../client && yarn build",
    "setup-db-node": "node setup-database.js",
    "setup-db": "./setup-database.sh",
    "prisma:generate": "yarn prisma generate",
    "prisma:migrate": "yarn prisma migrate dev",
    "prisma:migrate:deploy": "yarn prisma migrate deploy",
    "prisma:seed": "node prisma/seed.js"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "apollo-server-express": "^3.12.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "decimal.js": "^10.5.0",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "express-session": "^1.18.1",
    "graphql": "^16.8.1",
    "graphql-type-json": "^0.3.2",
    "moment": "^2.30.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.5",
    "react-table": "^7.8.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cross-env": "^10.1.0",
    "nodemon": "^2.0.15",
    "pm2": "^5.3.0",
    "prisma": "^5.5.2"
  }
}
